<%= simple_form_for(@score) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :result %>
    <%= f.association :user %>
    <%= f.association :lift %>
    <%= f.association :wod %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
