<%= simple_form_for(@wod) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :previous_wod, label: 'Previous WOD ID number' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
