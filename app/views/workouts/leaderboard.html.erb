
<!-- Workout.first.parts.first.wods -->

<% @workout.parts.each do |part| %>
	<% if part.style == 1 %> <!-- WOD -->
		<h3><%= part.wods.first.title %></h3>
		<% part.wods.first.scores.each do |score| %>
			<p><strong><%= score.user.email %>: </strong><%= score.result %> </p>
		<% end %>
	<% elsif part.style == 2 %> <!-- LIFT -->
	<h3><%= part.lifts.first.category.name %> </h3>
		<% part.lifts.first.scores.each do |score| %>
			<p><strong><%= score.user.email %>:</strong> <%= score.result %> </p>
		<% end %>
		<% else %> <!-- No Part Style (mess up) -->
	<% end %>
<% end %>
	
